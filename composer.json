{
    "name": "min/karthus",
    "description": "A simple Framework For Swoole",
    "type": "library",
    "version": "v2.0.8",
    "keywords": [
        "swoole",
        "framework",
        "async",
        "karthus",
        "php",
        "http",
        "http-server"
    ],
    "homepage": "https://git.blued.cn/min/karthus",
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.1",
        "psr/log": "^1.1.2",
        "nikic/fast-route": "^1.3",
        "psr/http-message": "^1.0.1",
        "firebase/php-jwt": "v5.0.0",
        "psr/container": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "php-di/phpdoc-reader": "^2.1",
        "doctrine/instantiator": "^1.3",
        "nikic/php-parser": "^4.3",
        "symfony/finder": "^4.4",
        "doctrine/annotations": "^1.8",
        "vlucas/phpdotenv": "^4.1",
        "psr/event-dispatcher": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0",
        "symfony/dotenv": "^4.4",
        "hassankhan/config": "^2.1",
        "phpunit/phpunit": "^8.5",
        "ext-redis": "*"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-mysqli": "*",
        "ext-zend-opcache": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-posix": "*",
        "ext-json": "*",
        "ext-apcu": ">=5.1.7",
        "ext-redis": "*",
        "ext-simplexml": "*",
        "ext-ctype": "*",
        "ext-mbstring": "*"
    },
    "license": "Apache License 2",
    "authors": [
        {
            "name": "Min",
            "email": "min@blued.com",
            "homepage": "https://git.blued.cn/min",
            "role": "Developer"
        },
        {
            "name": "Xu Zhaorong",
            "email": "xuzhaorong@blued.com",
            "homepage": "https://git.blued.cn/xuzhaorong",
            "role": "Developer"
        }
    ],
    "autoload": {
        "files": [
            "src/Functions.php"
        ],
        "psr-4": {
            "Karthus\\": "src/"
        }
    },
    "repositories": [{
        "type": "composer",
        "url": "https://composer.blued.cn"
    }],
    "support": {
        "issues": "https://git.blued.cn/min/karthus/issues",
        "email": "php@blued.com",
        "source": "https://git.blued.cn/min/karthus/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "bin": [
        "bin/karthus"
    ]
