{
    "name": "karthus/framework",
    "description": "A simple Framework For PHP",
    "type": "library",
    "version": "v3.2.0",
    "keywords": [
        "framework",
        "karthus",
        "php",
        "http",
        "router",
        "http-server"
    ],
    "homepage": "https://github.com/karhuts/framework",
    "require": {
        "php": ">=8.0",
        "composer/composer": "^2.6",
        "illuminate/database": "^11.36",
        "illuminate/events": "^11.36",
        "illuminate/pagination": "^11.36",
        "illuminate/redis": "^11.36",
        "laminas/laminas-diactoros": "^3.5",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "league/container": "^4.2",
        "monolog/monolog": "^3.8",
        "nikic/fast-route": "^1.3",
        "opis/closure": "^3.5",
        "psr/container": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.1",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "symfony/var-dumper": "^7.2",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.47",
        "phpunit/phpunit": "^10.4"
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Min",
            "email": "294953530@qq.com",
            "homepage": "https://github.com/minr",
            "role": "Master"
        }
    ],
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "karthus\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/karhuts/framework/issues",
        "email": "294953530@qq.com",
        "source": "https://github.com/karhuts/framework"
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "bin": [
        "bin/karthus"
    ]
}
