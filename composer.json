        "swoole/ide-helper": ">=4.6.4"
    },
    "license": "Apache License 2",
    "authors": [
        {
            "name": "Min",
            "email": "min@bluecity.com",
            "homepage": "https://git.ourbluecity.com/min",
            "role": "Developer"
        },
        {
            "name": "Xu Zhaorong",
            "email": "xuzhaorong@bluecity.com",
            "homepage": "https://git.ourbluecity.com/xuzhaorong",
            "role": "Developer"
        }
